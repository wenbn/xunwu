                h.HOUSE_STATUS = #{houseStatus,jdbcType=INTEGER},
            </if>
        </set>
        WHERE
        h.ID = #{id,jdbcType=INTEGER} and h.VERSION_NO = #{versionNo,jdbcType=INTEGER}
    </update>

    <select id="selectHousingApplicationCount" resultType="java.lang.Integer">
