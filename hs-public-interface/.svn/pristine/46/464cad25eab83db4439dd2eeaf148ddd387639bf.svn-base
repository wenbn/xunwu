package www.ucforward.com.index.entity;

/**
 * google地图实体
 * @author wenbn
 * @version 1.0
 * @date 2018/8/14
 */
public class GoogleMapLocation {

    // 经度
    private double lon;
    // 纬度
    private double lat;


    public double getLon() {
        return lon;
    }

    public void setLon(double lon) {
        this.lon = lon;
    }

    public double getLat() {
        return lat;
    }

    public void setLat(double lat) {
        this.lat = lat;
    }

    public GoogleMapLocation() {
    }

    public GoogleMapLocation(double lon, double lat) {
        this.lon = lon;
        this.lat = lat;
    }
}
